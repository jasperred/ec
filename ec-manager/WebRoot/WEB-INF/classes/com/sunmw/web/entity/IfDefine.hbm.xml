<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.web.entity">
	<class
		name="IfDefine"
		table="if_define"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="IF_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="IfCode"
			column="IF_code"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="IfName"
			column="IF_name"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="IfCat"
			column="IF_cat"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="Status"
			column="Status"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="ErrorRetryTimes"
			column="Error_retry_times"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="RetryWaitSeconds"
			column="Retry_wait_seconds"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="RunMethod"
			column="Run_method"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="ClassName"
			column="Class_name"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="IfType"
			column="If_type"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="ClassType"
			column="Class_type"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Ctime"
			column="ctime"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="Cuser"
			column="cuser"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Mtime"
			column="mtime"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="Muser"
			column="muser"
			type="string"
			not-null="false"
			length="30"
		/>


	</class>	
</hibernate-mapping>