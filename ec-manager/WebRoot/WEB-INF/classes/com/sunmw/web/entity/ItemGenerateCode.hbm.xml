<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.web.entity">
	<class
		name="ItemGenerateCode"
		table="item_generate_code"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="Brand"
			column="brand"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Cat"
			column="cat"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="SubCat"
			column="sub_cat"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Year"
			column="year"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Season"
			column="season"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Sex"
			column="sex"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Dept"
			column="dept"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Detail"
			column="detail"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Serial"
			column="serial"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="ItemCode"
			column="item_code"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="ItemName"
			column="item_name"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Sel"
			column="sel"
			type="string"
			not-null="false"
			length="5"
		/>
		<property
			name="ImageUrl"
			column="image_url"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Ctime"
			column="ctime"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="Mtime"
			column="mtime"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="Cuser"
			column="cuser"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="Muser"
			column="muser"
			type="string"
			not-null="false"
			length="60"
		/>


	</class>	
</hibernate-mapping>