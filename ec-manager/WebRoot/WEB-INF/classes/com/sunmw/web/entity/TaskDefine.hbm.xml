<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.web.entity">
	<class
		name="TaskDefine"
		table="task_define"
	>
		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="java.lang.Long"
			column="Task_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="IfId"
			column="IF_id"
			type="java.lang.Long"
			not-null="true"
			length="19"
		/>
		<property
			name="StartDate"
			column="Start_date"
			type="timestamp"
			not-null="true"
		/>
		<property
			name="EndDate"
			column="End_date"
			type="timestamp"
			not-null="true"
		/>
		<property
			name="LastStartTime"
			column="Last_start_time"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="LastEndTime"
			column="Last_end_time"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="LastSuccess"
			column="Is_last_success"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="CycleType"
			column="Cycle_type"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="Cycle"
			column="Cycle"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Status"
			column="Status"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="StoreId"
			column="store_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="NextExecTime"
			column="Next_exec_time"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="DayCycleType"
			column="Day_cycle_type"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="DayCycle"
			column="Day_cycle"
			type="time"
			not-null="false"
		/>
		<property
			name="DayStartTime"
			column="Day_start_time"
			type="time"
			not-null="false"
		/>
		<property
			name="DayEndTime"
			column="Day_end_time"
			type="time"
			not-null="false"
		/>
		<property
			name="Ctime"
			column="ctime"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="Cuser"
			column="cuser"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Mtime"
			column="mtime"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="Muser"
			column="muser"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="LastExecTime"
			column="Last_exec_time"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="CronExpression"
			column="cron_expression"
			type="string"
			not-null="false"
			length="200"
		/> 
		<property
			name="IsLoad"
			column="is_load"
			type="string"
			not-null="false"
			length="10"
		/>


	</class>	
</hibernate-mapping>