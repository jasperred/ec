<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.web.entity">
	<class
		name="Customer"
		table="customer"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="cust_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="CustNo"
			column="cust_no"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="CustNo1"
			column="cust_no1"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="CustName"
			column="cust_name"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Mobile"
			column="mobile"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Tel"
			column="tel"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="BirthDay"
			column="birth_day"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Email"
			column="email"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Sex"
			column="sex"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Province"
			column="province"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="City"
			column="city"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="District"
			column="district"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="Address"
			column="address"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Zipcode"
			column="zipcode"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="CompanyId"
			column="company_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Ctime"
			column="ctime"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="Cuser"
			column="cuser"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Mtime"
			column="mtime"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="Muser"
			column="muser"
			type="string"
			not-null="false"
			length="30"
		/>


	</class>	
</hibernate-mapping>