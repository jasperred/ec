<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.taobao.entity">
	<class
		name="TbProduct"
		table="tb_product"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="local_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="TbProductId"
			column="tb_product_id"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="OuterId"
			column="outer_id"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="Created"
			column="created"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="Tsc"
			column="tsc"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="Cid"
			column="cid"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="CatName"
			column="cat_name"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="Props"
			column="props"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="PropsStr"
			column="props_str"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="BindsStr"
			column="binds_str"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="SalePropsStr"
			column="sale_props_str"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="CollectNum"
			column="collect_num"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="Name"
			column="name"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="Binds"
			column="binds"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="SaleProps"
			column="sale_props"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="Price"
			column="price"
			type="big_decimal"
			not-null="false"
			length="18"
		/>
		<property
			name="Description"
			column="description"
			type="string"
			not-null="false"
		/>
		<property
			name="PicUrl"
			column="pic_url"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="Modified"
			column="modified"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="Status"
			column="status"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="Level"
			column="level"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="PicPath"
			column="pic_path"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="VerticalMarket"
			column="vertical_market"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="CustomerProps"
			column="customer_props"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="PropertyAlias"
			column="property_alias"
			type="string"
			not-null="false"
			length="512"
		/>


	</class>	
</hibernate-mapping>