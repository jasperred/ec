<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.taobao.entity">
	<class
		name="TbCatPropValue"
		table="tb_cat_prop_value"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="local_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="Cid"
			column="cid"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="Pid"
			column="pid"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="PropName"
			column="prop_name"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="Vid"
			column="vid"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="Name"
			column="name"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="NameAlias"
			column="name_alias"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="IsParent"
			column="is_parent"
			type="java.lang.String"
			not-null="false"
			length="5"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="SortOrder"
			column="sort_order"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>


	</class>	
</hibernate-mapping>