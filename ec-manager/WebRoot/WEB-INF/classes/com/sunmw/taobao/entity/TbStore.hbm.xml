<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.taobao.entity">
	<class
		name="TbStore"
		table="tb_store"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="store_id"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="StoreNick"
			column="store_nick"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="AppKey"
			column="app_key"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="AppSercet"
			column="app_sercet"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="SessionKey"
			column="session_key"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="StoreType"
			column="store_type"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="SandboxUrl"
			column="sandbox_url"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="SessionUrl"
			column="session_url"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="AreaId"
			column="area_id"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="Address"
			column="address"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="Zip"
			column="zip"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Phone"
			column="phone"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Mobile"
			column="mobile"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Memo"
			column="memo"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="UseNotify"
			column="use_notify"
			type="string"
			not-null="false"
			length="5"
		/>


	</class>	
</hibernate-mapping>