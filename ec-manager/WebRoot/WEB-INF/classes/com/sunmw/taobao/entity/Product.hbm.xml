<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.taobao.entity">
	<class
		name="Product"
		table="Product"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Integer"
			column="Product_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="Action"
			column="Action"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="PublishErrorMsg"
			column="Publish_error_msg"
			type="string"
			not-null="true"
			length="256"
		/>
		<property
			name="TbProductId"
			column="Tb_product_id"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="OuterId"
			column="Outer_id"
			type="string"
			not-null="false"
			length="36"
		/>
		<property
			name="Cid"
			column="Cid"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="Props"
			column="Props"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="PropsBack"
			column="Props_back"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="Name"
			column="Name"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="Binds"
			column="Binds"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="SaleProps"
			column="Sale_props"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="Price"
			column="Price"
			type="big_decimal"
			not-null="false"
			length="18"
		/>
		<property
			name="CustomerProps"
			column="Customer_props"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="PropertyAlias"
			column="Property_alias"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="CompanyId"
			column="company_id"
			type="java.lang.Integer"
			not-null="false"
			length="19"
		/>
		<property
			name="StoreId"
			column="Store_id"
			type="java.lang.Integer"
			not-null="false"
			length="19"
		/>
		<property
			name="UpdateTime"
			column="Update_time"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="CreateTime"
			column="Create_time"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="PublishTime"
			column="Publish_time"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="DefaultSku"
			column="Default_Sku"
			type="string"
			not-null="false"
			length="50"
		/>


	</class>	
</hibernate-mapping>