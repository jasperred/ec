<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.web.entity">
	<class
		name="SkuMaster"
		table="sku_master"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="sku_id"
		>
			<generator class="identity"/>
		</id>

		<many-to-one name="itemMaster" column="item_id" class="com.sunmw.web.entity.ItemMaster" ></many-to-one>
		<property
			name="SkuType"
			column="sku_type"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="SkuCd"
			column="sku_cd"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="SkuName"
			column="sku_name"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="SkuDesc"
			column="sku_desc"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="Barcode1"
			column="barcode1"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="Barcode2"
			column="barcode2"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="SkuStatus"
			column="sku_status"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="SkuPrice1"
			column="sku_price1"
			type="big_decimal"
			not-null="false"
			length="18"
		/>
		<property
			name="SkuPrice2"
			column="sku_price2"
			type="big_decimal"
			not-null="false"
			length="18"
		/>
		<property
			name="SkuProp1"
			column="sku_prop1"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="SkuProp2"
			column="sku_prop2"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="SkuProp3"
			column="sku_prop3"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="SkuProp4"
			column="sku_prop4"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="Receive1"
			column="receive1"
			type="string"
			not-null="false"
			length="45"
		/>
		<property
			name="Receive2"
			column="receive2"
			type="string"
			not-null="false"
			length="45"
		/>
		<property
			name="Receive3"
			column="receive3"
			type="string"
			not-null="false"
			length="45"
		/>
		<property
			name="Receive4"
			column="receive4"
			type="string"
			not-null="false"
			length="45"
		/>
		<property
			name="Receive5"
			column="receive5"
			type="string"
			not-null="false"
			length="45"
		/>
		<property
			name="Receive6"
			column="receive6"
			type="string"
			not-null="false"
			length="45"
		/>
		<property
			name="Ctime"
			column="ctime"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="Mtime"
			column="mtime"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="ReserveInv"
			column="reserve_inv"
			type="integer"
			not-null="false"
			length="10"
		/>


	</class>	
</hibernate-mapping>