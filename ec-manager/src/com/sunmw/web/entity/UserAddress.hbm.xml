<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.web.entity">
	<class
		name="UserAddress"
		table="user_address"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="address_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="AddressName"
			column="address_name"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="UserId"
			column="user_id"
			type="integer"
			not-null="true"
			length="8"
		/>
		<property
			name="Consignee"
			column="consignee"
			type="string"
			not-null="true"
			length="60"
		/>
		<property
			name="Email"
			column="email"
			type="string"
			not-null="true"
			length="60"
		/>
		<property
			name="Country"
			column="country"
			type="string"
			not-null="true"
			length="60"
		/>
		<property
			name="Province"
			column="province"
			type="string"
			not-null="true"
			length="60"
		/>
		<property
			name="City"
			column="city"
			type="string"
			not-null="true"
			length="60"
		/>
		<property
			name="District"
			column="district"
			type="string"
			not-null="true"
			length="60"
		/>
		<property
			name="Address"
			column="address"
			type="string"
			not-null="true"
			length="256"
		/>
		<property
			name="Zipcode"
			column="zipcode"
			type="string"
			not-null="true"
			length="60"
		/>
		<property
			name="Tel"
			column="tel"
			type="string"
			not-null="true"
			length="60"
		/>
		<property
			name="Mobile"
			column="mobile"
			type="string"
			not-null="true"
			length="60"
		/>
		<property
			name="Ctime"
			column="ctime"
			type="timestamp"
			not-null="false"
			length="10"
		/>
		<property
			name="Mtime"
			column="mtime"
			type="timestamp"
			not-null="false"
			length="10"
		/>
		<property
			name="Cuser"
			column="cuser"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Muser"
			column="muser"
			type="string"
			not-null="false"
			length="10"
		/>


	</class>	
</hibernate-mapping>