<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.taobao.entity">
	<class
		name="ProductImg"
		table="ProductImg"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Integer"
			column="Product_Img_ID"
		>
			<generator class="identity"/>
		</id>

		<property
			name="ProductId"
			column="Product_id"
			type="java.lang.Integer"
			not-null="false"
			length="19"
		/>
		<property
			name="ImgIndex"
			column="Img_Index"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="ImgUrl"
			column="Img_URL"
			type="string"
			not-null="false"
			length="1024"
		/>
		<property
			name="TbProductImgId"
			column="Tb_product_img_id"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>


	</class>	
</hibernate-mapping>