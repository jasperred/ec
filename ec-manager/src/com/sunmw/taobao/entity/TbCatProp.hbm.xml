<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.taobao.entity">
	<class
		name="TbCatProp"
		table="tb_cat_prop"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="local_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="Pid"
			column="pid"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="ParentPid"
			column="parent_pid"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="ParentVid"
			column="parent_vid"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="Name"
			column="name"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="IsKeyProp"
			column="is_key_prop"
			type="java.lang.String"
			not-null="false"
			length="5"
		/>
		<property
			name="IsSaleProp"
			column="is_sale_prop"
			type="java.lang.String"
			not-null="false"
			length="5"
		/>
		<property
			name="IsColorProp"
			column="is_color_prop"
			type="java.lang.String"
			not-null="false"
			length="5"
		/>
		<property
			name="IsEnumProp"
			column="is_enum_prop"
			type="java.lang.String"
			not-null="false"
			length="5"
		/>
		<property
			name="IsInputProp"
			column="is_input_prop"
			type="java.lang.String"
			not-null="false"
			length="5"
		/>
		<property
			name="IsItemProp"
			column="is_item_prop"
			type="java.lang.String"
			not-null="false"
			length="5"
		/>
		<property
			name="Must"
			column="must"
			type="java.lang.String"
			not-null="false"
			length="5"
		/>
		<property
			name="Multi"
			column="multi"
			type="java.lang.String"
			not-null="false"
			length="5"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="SortOrder"
			column="sort_order"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="ChildTemplate"
			column="child_template"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="IsAllowAlias"
			column="is_allow_alias"
			type="java.lang.String"
			not-null="false"
			length="5"
		/>


	</class>	
</hibernate-mapping>