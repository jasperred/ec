<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.taobao.entity">
	<class
		name="TbBrand"
		table="tb_brand"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="local_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="Vid"
			column="vid"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="Name"
			column="name"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="Pid"
			column="pid"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="PropName"
			column="prop_name"
			type="string"
			not-null="false"
			length="64"
		/>


	</class>	
</hibernate-mapping>