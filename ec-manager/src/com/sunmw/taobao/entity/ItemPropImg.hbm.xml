<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.taobao.entity">
	<class
		name="ItemPropImg"
		table="ItemPropImg"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Integer"
			column="Item_Prop_Img_ID"
		>
			<generator class="identity"/>
		</id>

		<property
			name="TbItemPropImgId"
			column="Tb_item_prop_img_id"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="ItemId"
			column="Item_id"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="PropId"
			column="Prop_id"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="ValueId"
			column="Value_id"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="ImgUrl"
			column="Img_URL"
			type="string"
			not-null="false"
			length="1024"
		/>


	</class>	
</hibernate-mapping>